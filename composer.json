{
    "name": "roy688/laravel-querylist",
    "description": "Laravel5 facade for QueryList. Simple, elegant, extensible PHP Web Scraper (crawler/spider),Use the css3 dom selector,Based on phpQuery!",
    "type": "laravel-packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Roy688",
            "email": "slaythia@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "jaeger/querylist": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Roy688\\QueryList\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roy688\\QueryList\\QueryListProvider"
            ],  
            "aliases": {
                "QueryList": "Roy688\\QueryList\\QueryListFacade"
            }
        }
    }
}
